def cal_area(w,h):
  return w * h


h = int(input("Enter int height h:"))
print (f'Hello World h = {h:5d}')

w = int(input("Enter int width w:"))
print (f'Hello World w = {w:5d}')

print("area = ",cal_area(w,h))
